image{
  width:32px;
  height:36px;
  display:inline-block;
  margin-left:15px;
}
input{
  display:inline-block;
  width:70%;
  padding:5px;
  font-size:14px;
  background-color:#F6F7F9;
}
button{
  width:50px;
  height:37px;
  padding:5px;
  font-size:16px;
  line-height:26px;
  color:#aaa;
  background-color:#fff;
  display:inline-block;
  border:1px solid #fff;
}
.dajdz{
  background-color:#F7F7F7;
  font-size:14px;
  width:100%;
  height:40px;
}
.dajdz>text{
  margin-left:20px;
  line-height:50px;
  color:#969BA3;
}
.biaoq{
    margin:15rpx 30rpx;
    display:flex;
    flex-wrap: wrap;
    justify-content: center;
    margin-bottom:20px;
}
.biaoq text{
    font-size:14px; 
    color: #969ba3;
    margin:8px;
    border:1px solid #969ba3;
    padding:6rpx 10rpx;
    border-radius:10px;
}